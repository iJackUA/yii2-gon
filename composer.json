{
  "name": "ijackua/yii2-gon",
  "description": "Push data from PHP controller to JS variable (inspired by https://github.com/gazay/gon)",
  "keywords": [
    "javascript",
    "yii",
    "extension"
  ],
  "type": "yii2-extension",
  "homepage": "https://github.com/iJackUA/yii2-gon",
  "license": "MIT",
  "authors": [
    {
      "name": "Ievgen Kuzminov",
      "email": "kyzminov@gmail.com",
      "homepage": "http://stdout.in",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "yiisoft/yii2": "*"
  },
  "autoload": {
    "psr-4": {
      "ijackua\\gon\\": "src"
    }
  },
  "extra": {
    "bootstrap": "ijackua\\gon\\GonBootstrap",
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
